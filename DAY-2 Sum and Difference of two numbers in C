#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{    int a=10, b=4, sum1=0, sub1=0;
     float c=4.0,d=2.0, sum2=0, sub2=0;
     
     scanf("%d" , &a);
     scanf("%d", &b);
     scanf("%f", &c);
     scanf("%f", &d);
     
     sum1 = a+b;
     sub1 = a-b;
     sum2 = c+d;
     sub2 = c-d;
     
     printf("%d %d\n", sum1, sub1);
     printf("%0.1f %0.1f", sum2 , sub2);
        
	
    
    return 0;
}
