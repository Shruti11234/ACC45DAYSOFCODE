#include <stdio.h>
#include <iostream>
using namespace std;


int main() {
    int num1, num2, *ptr1, *ptr2, sum=0,dif=0;
   
cin>>num1>>num2;
    ptr1 = &num1;
    ptr2 = &num2;
    sum = *ptr1 + *ptr2;
    dif = *ptr1 - *ptr2;
    if (dif<0) {
    dif=-(dif);
    }
    cout<<sum<<endl;
    cout<<dif;
    return 0;
