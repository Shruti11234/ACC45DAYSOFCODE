#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    char ch, si[20], sen[20];
    scanf("%c", &ch);
    scanf("%s\n", si);  
    scanf("%[^\n]%*c\n", sen);
      
    printf("%c\n", ch);
    printf("%s\n", si);
    printf("%s\n", sen);
         
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}


