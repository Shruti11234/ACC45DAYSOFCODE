#include <math.h>
#include <stdlib.h>

int main() {
	
    int n;
    scanf("%d", &n);
    //Complete the code to calculate the sum of the five digits on n.
    
    int sum=0,x;
    while(n!=0)
    {
       x = n%10;
       sum = sum+x;
       n = n/10;
    }
    printf("%d", sum);    
    return 0;
}
